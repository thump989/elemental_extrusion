{
	"format_version": "1.19.0",
	"animation_controllers": {
		"controller.animation.underworld_hands.move_controller": {
			"initial_state": "idle",
			"states": {
				"idle": {
					"transitions": [
						{"walking": "q.ground_speed > 0.25"}
					],
					"blend_transition": 0.2
				},
				"walking": {
					"animations": ["move"],
					"transitions": [
						{"idle": "q.ground_speed < 0.25"}
					],
					"blend_transition": 0.2
				}
			}
		}
	}
}