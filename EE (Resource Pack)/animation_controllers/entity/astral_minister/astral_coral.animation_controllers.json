{
	"format_version": "1.19.0",
	"animation_controllers": {
		"controller.animation.astral_coral.controller": {
			"states": {
				"default": {
					"animations": ["spawn"],
					"transitions": [
						{"idle": "q.all_animations_finished"}
					]
				},
				"idle": {
					"animations": ["idle"],
					"transitions": [
						{"attack": "v.attack_time>0"}
					]
				},
				"attack": {
					"animations": ["attack"],
					"transitions": [
						{"idle": "q.all_animations_finished"}
					]
				}
			}
		}
	}
}