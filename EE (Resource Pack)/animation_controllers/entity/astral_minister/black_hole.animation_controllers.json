{
	"format_version": "1.19.0",
	"animation_controllers": {
		"controller.animation.black_hole.cube": {
			"states": {
				"default": {
					"animations": ["spawn"],
					"sound_effects": [
						{"effect": "suction"}
					],
					"transitions": [
						{"suction": "q.property('thump989:suction')==true"}
					]
				},
				"suction": {
					"animations": ["suction_cube"],
					"transitions": [
						{"end": "q.property('thump989:suction')==false"}
					],
					"blend_transition": 2,
					"blend_via_shortest_path": true
				},
				"end": {
					"animations": ["cube_end"]
				}
			}
		},
		"controller.animation.black_hole.partice": {
			"states": {
				"default": {
					"transitions": [
						{"particle": "q.property('thump989:suction')==true"}
					]
				},
				"particle": {
					"particle_effects": [
						{
							"effect": "black_hole",
							"locator": "locator"
						}
					],
					"transitions": [
						{"default": "q.property('thump989:suction')==false"}
					]
				}
			}
		}
	}
}