{
	"format_version": "1.19.0",
	"animation_controllers": {
		"controller.animation.radioactive_shrimp.water_controller": {
			"initial_state": "idle",
			"states": {
				"idle": {
					"animations": ["head_idle", "body_idle"],
					"transitions": [
						{"walking": "q.ground_speed > 1.0"}
					],
					"blend_transition": 0.6,
					"blend_via_shortest_path": true
				},
				"walking": {
					"animations": ["body_swim", "legs_small_swim", "legs_large_swim", "head_idle"],
					"transitions": [
						{"idle": "q.ground_speed < 0.5"}
					],
					"blend_transition": 0.6
				}
			}
		},
		"controller.animation.radioactive_shrimp.ground_controller": {
			"initial_state": "idle",
			"states": {
				"idle": {
					"animations": ["head_idle", "land_idle"],
					"transitions": [
						{"walking": "q.ground_speed > 1.0"}
					],
					"blend_transition": 0.6,
					"blend_via_shortest_path": true
				},
				"walking": {
					"animations": ["body_swim", "legs_small_swim", "legs_large_swim", "head_idle", "land_idle"],
					"transitions": [
						{"idle": "q.ground_speed < 0.5"}
					],
					"blend_transition": 0.6
				}
			}
		},
		"controller.animation.radioactive_shrimp.root_controller": {
			"states": {
				"default": {
					"animations": ["water_controller"],
					"transitions": [
						{"land": "!q.is_in_water"}
					],
					"blend_transition": 0.2,
					"blend_via_shortest_path": true
				},
				"land": {
					"animations": ["ground_controller"],
					"transitions": [
						{"default": "q.is_in_water"}
					],
					"blend_transition": 0.2,
					"blend_via_shortest_path": true
				}
			}
		}
	}
}