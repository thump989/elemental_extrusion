{
	"format_version": "1.19.0",
	"animation_controllers": {
		"controller.animation.titan_beetle.walk": {
			"initial_state": "idle",
			"states": {
				"idle": {
					"transitions": [
						{"walking": "q.ground_speed > 1.0"}
					],
					"blend_transition": 0.2
				},
				"walking": {
					"animations": ["walk"],
					"transitions": [
						{"idle": "q.ground_speed < 0.5"}
					],
					"blend_transition": 0.2
				}
			}
		},
		"controller.animation.titan_beetle.walk_controller": {
			"initial_state": "idle",
			"states": {
				"idle": {
					"transitions": [
						{"walking": "q.ground_speed > 0.6"}
					],
					"blend_transition": 0.2
				},
				"walking": {
					"animations": ["walk"],
					"transitions": [
						{"idle": "q.ground_speed < 0.5"}
					],
					"blend_transition": 0.2
				}
			}
		},
		"controller.animation.titan_beetle.gravid_controller": {
			"states": {
				"default": {
					"animations": ["wings_idle"],
					"transitions": [
						{"gravid": "q.is_pregnant"}
					],
					"blend_transition": 0.2,
					"blend_via_shortest_path": true
				},
				"gravid": {
					"animations": ["gravid"],
					"transitions": [
						{"default": "!q.is_pregnant"}
					],
					"blend_transition": 0.2,
					"blend_via_shortest_path": true
				}
			}
		},
		"controller.animation.titan_beetle.root_controller": {
			"states": {
				"default": {
					"animations": ["head_idle", "mandible_controller", "walk_controller", "gravid_controller"]
				}
			}
		},
		"controller.animation.titan_beetle.mandible_controller": {
			"states": {
				"default": {
					"animations": ["mandible_idle", "noloot"],
					"transitions": [
						{"loot": "q.property('thump989:hunger_level')==16"}
					]
				},
				"loot": {
					"animations": ["mandible_loot"],
					"transitions": [
						{"default": "q.property('thump989:hunger_level')<16"}
					]
				}
			}
		}
	}
}