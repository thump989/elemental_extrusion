{
  "format_version": "1.10.0",
  "render_controllers": {
    "controller.render.shooting_star": {
      "ignore_lighting": true,
      "geometry": "Geometry.default",
      "textures": ["Texture.default"],
      "materials": [
        {
          "*": "Material.default"
        }
      ],
      "uv_anim": {
        "scale": [1, "1 / 8"],
        "offset": [0, "Math.mod(Math.floor(query.life_time * 15), 8) / 8"]
      }
    },
    "controller.render.shootig_star_outer": {
      "ignore_lighting": true,
      "geometry": "Geometry.outer",
      "textures": ["Texture.outer"],
      "materials": [
        {
          "*": "Material.outer"
        }
      ]
    }
  }
}
