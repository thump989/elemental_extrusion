{
  "format_version": "1.10.0",
  "render_controllers": {
    "controller.render.evil_skull": {
      "geometry": "Geometry.default",
      "textures": ["Texture.default"],
      "materials": [
        {
          "*": "Material.default"
        }
      ],
      "ignore_lighting": true,
      "uv_anim": {
        "scale": [1, "1 / 7"],
        "offset": [0, "Math.mod(Math.floor(query.life_time * 10), 7) / 7"]
      }
    },
    "controller.render.shadowfire": {
      "geometry": "Geometry.default",
      "textures": ["Texture.default"],
      "ignore_lighting": true,
      "materials": [
        {
          "*": "Material.default"
        }
      ],
      "uv_anim": {
        "scale": [1, "1 / 32"],
        "offset": [0, "Math.mod(Math.floor(query.life_time * 25), 32) / 32"]
      }
    },
    "controller.render.effect_inner": {
      "geometry": "Geometry.default",
      "arrays": {
        "textures": {
          "array.effect_variant": [
            "texture.default",
            "texture.weakness",
            "texture.weaving",
            "texture.darkness",
            "texture.hunger",
            "texture.trialomen",
            "texture.wither"
          ]
        }
      },
      "textures": [
        "array.effect_variant[query.property('thump989:effect_variant')]"
      ],
      "ignore_lighting": true,
      "materials": [
        {
          "*": "Material.default"
        }
      ]
    },
    "controller.render.cube_outer": {
      "geometry": "Geometry.outer",
      "arrays": {
        "textures": {
          "array.effect_variant": [
            "texture.default",
            "texture.weakness",
            "texture.weaving",
            "texture.darkness",
            "texture.hunger",
            "texture.trialomen",
            "texture.wither"
          ]
        }
      },
      "textures": [
        "array.effect_variant[query.property('thump989:effect_variant')]"
      ],
      "ignore_lighting": true,
      "materials": [
        {
          "*": "Material.outer"
        }
      ]
    }
  }
}
