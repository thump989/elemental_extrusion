{
  "format_version": "1.21.70",
  "minecraft:entity": {
    "description": {
      "identifier": "thump989:titan_beetle",
      "is_spawnable": true,
      "is_summonable": true,
      "is_experimental": false,
      "properties": {
        "thump989:hunger_level": {
          "client_sync": true,
          "default": 0,
          "range": [0, 16],
          "type": "int"
        }
      }
    },
    "components": {
      "minecraft:physics": {
        "has_gravity": true,
        "has_collision": true
      },
      "minecraft:health": {
        "value": 8,
        "max": 8
      },
      "minecraft:experience_reward": {
        "on_bred": 7,
        "on_death": "q.last_hit_by_player ? math.random(1,3) : 0"
      },
      "minecraft:breathable": {
        "total_supply": 15,
        "breathes_air": true,
        "suffocate_time": -20
      },
      "minecraft:behavior.float": {
        "priority": 0
      },
      "minecraft:leashable": {
        "can_be_stolen": false,
        "hard_distance": 6,
        "max_distance": 10,
        "soft_distance": 4
      },
      "minecraft:type_family": {
        "family": ["mob", "titan_beetle"]
      },
      "minecraft:collision_box": {
        "height": 0.6,
        "width": 0.6
      },
      "minecraft:movement": {
        "value": 0.2
      },
      "minecraft:navigation.walk": {
        "avoid_water": true,
        "can_path_over_water": false
      },
      "minecraft:movement.generic": {
        "max_turn": 30
      },
      "minecraft:jump.static": {
        "jump_power": 0.42
      },
      "minecraft:nameable": {
        "allow_name_tag_renaming": true,
        "always_show": false
      },
      "minecraft:behavior.random_stroll": {
        "interval": 120,
        "priority": 4,
        "speed_multiplier": 0.8,
        "xz_dist": 10,
        "y_dist": 7
      },
      "minecraft:pushable": {
        "is_pushable": true
      },
      "minecraft:push_through": {
        "value": 0
      }
    },
    "component_groups": {
      "self:loot_ready": {
        "minecraft:loot": {
          "table": "loot_tables/entity/titan_beetle_loot.json"
        },
        "minecraft:interact": {
          "interactions": [
            {
              "spawn_items": {
                "table": "loot_tables/entity/titan_beetle_loot.json"
              },
              "cooldown": 5,
              "on_interact": {
                "target": "self",
                "event": "thump989:loot_gave"
              }
            }
          ]
        }
      },
      "self:hungry": {
        "minecraft:environment_sensor": {
          "triggers": [
            {
              "event": "thump989:loot_ready",
              "target": "self",
              "filters": {
                "test": "int_property",
                "value": 16,
                "operator": "==",
                "subject": "self",
                "domain": "thump989:hunger_level"
              }
            }
          ]
        },
        "minecraft:behavior.move_to_block": {
          "start_chance": 0.5,
          "search_range": 16,
          "goal_radius": 1,
          "stay_duration": 2,
          "priority": 6,
          "search_height": 16,
          "target_blocks": [
            "minecraft:leaf_litter",
            "minecraft:red_mushroom",
            "minecraft:brown_mushroom"
          ]
        },
        "minecraft:interact": {
          "interactions": [
            {
              "on_interact": {
                "event": "thump989:force_fed",
                "filters": {
                  "all_of": [
                    {
                      "any_of": [
                        {
                          "test": "has_equipment",
                          "subject": "other",
                          "domain": "hand",
                          "value": "minecraft:leaf_litter"
                        },
                        {
                          "test": "has_equipment",
                          "subject": "other",
                          "domain": "hand",
                          "value": "minecraft:leaves"
                        }
                      ]
                    },
                    {
                      "test": "is_family",
                      "subject": "other",
                      "value": "player"
                    }
                  ]
                }
              },
              "play_sounds": "eat",
              "cooldown": 0,
              "swing": true,
              "health_amount": 1,
              "use_item": true
            }
          ]
        },
        "minecraft:behavior.eat_block": {
          "time_until_eat": 0,
          "priority": 7,
          "on_eat": {
            "target": "self",
            "event": "thump989:block_eaten"
          },
          "success_chance": 0.04,
          "eat_and_replace_block_pairs": [
            {
              "eat_block": "minecraft:leaf_litter",
              "replace_block": "minecraft:air"
            },
            {
              "eat_block": "minecraft:brown_mushroom",
              "replace_block": "minecraft:air"
            },
            {
              "eat_block": "minecraft:red_mushroom",
              "replace_block": "minecraft:air"
            }
          ]
        }
      },
      "self:breedable": {
        "minecraft:behavior.tempt": {
          "priority": 4,
          "speed_multiplier": 1.25,
          "items": ["brown_mushroom", "red_mushroom"]
        },
        "minecraft:breedable": {
          "require_tame": false,
          "breed_items": ["brown_mushroom", "red_mushroom"],
          "breeds_with": [
            {
              "mate_type": "thump989:titan_beetle",
              "baby_type": "thump989:titan_beetle",
              "breed_event": {
                "event": "thump989:get_pregnant",
                "target": "self"
              }
            }
          ],
          "causes_pregnancy": true,
          "allow_sitting": true
        },
        "minecraft:behavior.breed": {
          "priority": 1,
          "speed_multiplier": 1
        }
      },
      "self:gravid": {
        "minecraft:behavior.lay_egg": {
          "goal_radius": 1.5,
          "egg_type": "thump989:titan_beetle_egg",
          "use_default_animation": true,
          "target_materials_above_block": "Air",
          "search_height": 16,
          "search_range": 16,
          "lay_seconds": 10,
          "priority": 2,
          "target_blocks": [
            "minecraft:podzol",
            "mienrcaft:spruce_log",
            "minecraft:stripped_spruce_log"
          ],
          "on_lay": {
            "event": "thump989:egg_layed",
            "target": "self"
          }
        },
        "minecraft:is_pregnant": {}
      },
      "self:is_baby": {
        "minecraft:ageable": {
          "duration": 1200,
          "grow_up": {
            "event": "thump989:become_adult",
            "target": "self"
          },
          "feed_items": ["red_mushroom", "brown_mushroom"]
        },
        "minecraft:scale_by_age": {
          "end_scale": 1,
          "start_scale": 0.2
        },
        "minecraft:behavior.tempt": {
          "priority": 4,
          "speed_multiplier": 1.25,
          "items": ["brown_mushroom", "red_mushroom"]
        }
      },
      "self:natural": {
        "minecraft:despawn": {
          "despawn_from_distance": {}
        }
      }
    },
    "events": {
      "thump989:force_fed": {
        "set_property": {
          "thump989:hunger_level": "q.property('thump989:hunger_level')+1"
        }
      },
      "thump989:block_eaten": {
        "set_property": {
          "thump989:hunger_level": "q.property('thump989:hunger_level')+2"
        }
      },
      "thump989:loot_ready": {
        "remove": {
          "component_groups": ["self:hungry"]
        },
        "add": {
          "component_groups": ["self:loot_ready"]
        }
      },
      "thump989:loot_gave": {
        "set_property": {
          "thump989:hunger_level": 0
        },
        "add": {
          "component_groups": ["self:hungry"]
        },
        "remove": {
          "component_groups": ["self:loot_ready"]
        }
      },
      "thump989:become_adult": {
        "remove": {
          "component_groups": ["self:is_baby"]
        },
        "add": {
          "component_groups": ["self:breedable", "self:hungry"]
        }
      },
      "minecraft:entity_born": {
        "add": {
          "component_groups": ["self:is_baby"]
        }
      },
      "minecraft:entity_spawned": {
        "add": {
          "component_groups": ["self:breedable", "self:natural", "self:hungry"]
        }
      },
      "thump989:get_pregnant": {
        "add": {
          "component_groups": ["self:gravid"]
        },
        "remove": {
          "component_groups": [
            "self:breedable",
            "self:hungry",
            "self:loot_ready"
          ]
        }
      },
      "thump989:egg_layed": {
        "remove": {
          "component_groups": ["self:gravid"]
        },
        "add": {
          "component_groups": ["self:breedable", "self:hungry"]
        }
      }
    }
  }
}
