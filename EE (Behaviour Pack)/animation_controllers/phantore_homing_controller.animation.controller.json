{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.phantore_homing": {
      "states": {
        "default": {
          "transitions": [
            {
              "skull_attack_1": "query.property('thump989:phantore_attack_state')=='homing'"
            }
          ]
        },
        "skull_attack_1": {
          "animations": ["homing_attack"],
          "transitions": [
            {
              "default": "!query.property('thump989:phantore_attack_state')=='homing' || q.all_animations_finished"
            }
          ]
        }
      }
    }
  }
}
